{
  "name": "sibagome/php-ci-demo",
  "description": "PHP CI/CD Demo Project with MySQL",
  "type": "project",
  "require": {
      "php": "^8.1",
      "ext-pdo": "*",
      "ext-pdo_mysql": "*"
  },
  "require-dev": {
      "phpunit/phpunit": "^10.0"
  },
  "autoload": {
      "psr-4": {
          "App\\": "src/"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Tests\\": "tests/"
      }
  }
}
